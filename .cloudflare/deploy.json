{
  "deploy": {
    "workers": [
      {
        "name": "table-to-image-mcp",
        "path": ".",
        "deploy_args": {
          "compatibility_date": "2024-01-01",
          "vars": {
            "MAX_TABLE_SIZE": "10000",
            "DEFAULT_IMAGE_FORMAT": "png",
            "DEFAULT_IMAGE_WIDTH": "800",
            "DEFAULT_IMAGE_HEIGHT": "600",
            "RATE_LIMIT": "60",
            "ENABLE_CORS": "true",
            "LOG_LEVEL": "info"
          }
        }
      }
    ]
  }
}
