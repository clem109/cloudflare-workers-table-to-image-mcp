        ["China", "1.4B", "$17T", "9.6M km²"],
        ["India", "1.4B", "$3.5T", "3.3M km²"],
        ["Japan", "125M", "$5T", "377K km²"]
      ],
      "useCases": [
        "CSV file data",
        "Excel exports",
        "Simple data tables"
      ]
    }
