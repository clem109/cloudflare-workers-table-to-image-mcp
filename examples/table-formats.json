{
  "description": "Examples of supported table data formats",
  "formats": {
    "format1_headers_rows": {
      "name": "Headers + Rows Format",
      "description": "Table with explicit headers and rows arrays",
      "example": {
        "headers": ["Product", "Q1 Sales", "Q2 Sales", "Growth"],
        "rows": [
          ["Widget A", "$50,000", "$65,000", "+30%"],
          ["Widget B", "$75,000", "$82,000", "+9%"],
          ["Widget C", "$30,000", "$45,000", "+50%"]
        ]
      },
      "useCases": [
        "Database query results",
        "Spreadsheet exports",
        "Structured reports"
      ]
    },
    "format2_array_of_objects": {
      "name": "Array of Objects Format",
      "description": "Array where each element is an object with key-value pairs",
      "example": [
        {
          "name": "Alice Johnson",
          "department": "Engineering",
          "salary": "$120,000",
          "years": 5
        },
        {
          "name": "Bob Smith",
          "department": "Marketing",
          "salary": "$95,000",
          "years": 3
        },
        {
          "name": "Carol White",
          "department": "Sales",
          "salary": "$110,000",
          "years": 7
        }
      ],
      "useCases": [
        "JSON API responses",
        "MongoDB query results",
        "JavaScript data structures"
      ]
    },
    "format3_2d_array": {
      "name": "2D Array Format",
      "description": "Simple 2D array where first row is headers",
      "example": [
        ["Country", "Population", "GDP", "Area"],
