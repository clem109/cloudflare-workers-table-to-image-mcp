{
  "name": "cloudflare-workers-table-to-image-mcp",
  "version": "1.0.0",
  "description": "Convert table data to images using QuickChart.io API - MCP connector for Cloudflare Workers",
  "main": "src/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:production": "wrangler deploy --env production",
    "deploy:staging": "wrangler deploy --env staging",
    "tail": "wrangler tail",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.js\"",
    "secret:set": "wrangler secret put"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "table",
    "image",
    "chart",
    "quickchart",
    "mcp",
    "poke",
    "api",
    "edge",
    "serverless"
  ],
  "author": "Clement Venard",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clem109/cloudflare-workers-table-to-image-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/clem109/cloudflare-workers-table-to-image-mcp/issues"
  },
  "homepage": "https://github.com/clem109/cloudflare-workers-table-to-image-mcp#readme",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240620.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "vitest": "^1.6.0",
    "wrangler": "^3.60.0"
  },
  "dependencies": {},
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}